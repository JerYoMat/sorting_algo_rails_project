<!DOCTYPE html>
<html>
  <head>
    <%= favicon_link_tag asset_path('favicon.ico') %>
    <title><%= full_title(yield(:title)) %></title>
    <%= render '/layouts/rails_default'%>
    <%= render 'shared/shim'%>
  </head>
    <%= render '/layouts/header' %>
  <body>
    <div class="container">
      <%= render 'shared/flash'%>
